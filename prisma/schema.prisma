generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:4321@mysqldb:3306/sorteio"
}

model Guest {
  id        String   @id @default(cuid())
  name      String @db.VarChar(100)

  @@map("guest")
}
model List {
  id        String     @id @default(cuid())
  items      String @db.LongText

  @@map("list")
}

model Sorteio {
  id        String     @id @default(cuid())
  list_id String? 
  guest_id     String?

  @@map("sorteio")
}





